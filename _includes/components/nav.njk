<nav>
  {% set navPages = collections.all | eleventyNavigation %}
  <ul class="flex flex-row flex-wrap">
  {%- for entry in navPages %}
    <li>
      <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item"{% endif %} class="pr-4 md:text-xl hover:text-red-400 ">{{ entry.title }}</a>
    </li>
  {%- endfor %}
  </ul>
</nav>
